{
  "name": "deviations",
  "version": "2.0.0",
  "description": "Deviation application",
  "scripts": {
    "prestart": "babel-node bin/startMessage.js",
    "clean": "rimraf dist",
    "build": "webpack --progress --verbose --colors --display-error-details --config webpack/common.config.js",
    "build:production": "npm run clean && npm run build",
    "lint": "node_modules/.bin/esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "NODE_ENV=development babel-node server.js",
    "startProduction": "NODE_ENV=production babel-node server.js",
    "test:watch": "babel-node bin/testMessage",
    "dpGit": "git add . && git commit -m 'eslint / refactor' && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielpoulson/deviations.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "babel6",
    "boilerplate",
    "redux",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "body-parser": "^1.15.0",
    "colors": "^1.1.2",
    "cookie-parser": "^1.3.5",
    "crypto": "0.0.3",
    "ejs": "^2.3.3",
    "express": "^4.13.3",
    "express-csv": "^0.6.0",
    "express-session": "^1.11.3",
    "foreman": "^2.0.0",
    "json2csv": "^3.2.0",
    "lodash": "^3.10.1",
    "moment": "^2.12.0",
    "mongoose": "^4.4.11",
    "mongoose-to-csv": "^0.1.0",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "nodemailer": "^2.3.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0"
  }
}
